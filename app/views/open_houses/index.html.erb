<p id="notice"><%= notice %></p>

<h1>Listing Open Houses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @open_houses.each do |open_house| %>
      <tr>
        <td><%= link_to 'Show', open_house %></td>
        <td><%= link_to 'Edit', edit_open_house_path(open_house) %></td>
        <td><%= link_to 'Destroy', open_house, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Open house', new_open_house_path %>
